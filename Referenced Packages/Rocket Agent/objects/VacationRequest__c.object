    <validationRules>
        <fullName>Validation_End_date_and_current_date</fullName>
        <active>false</active>
        <description>Vacation end date must be greater than current date</description>
        <errorConditionFormula>StatusFormula__c == &apos;Cancelled&apos;</errorConditionFormula>
        <errorMessage>Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Initiator_and_Assignee_roles</fullName>
        <active>false</active>
        <description>Initiator and Assignee must have the same roles</description>
        <errorConditionFormula>&quot;rca__Assignee__r.FirstName    ==  rca__Vacationer__r.UserRole.Name&quot; == NULL</errorConditionFormula>
        <errorMessage>Initiator and Assignee must have the same roles</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Start_Date</fullName>
        <active>false</active>
        <description>Validation Start Date</description>
        <errorConditionFormula>VRStartDateAndTime__c &lt;= now()+1/24/60</errorConditionFormula>
        <errorMessage>Start Date and time should not be life in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Start_and_End_Date</fullName>
        <active>true</active>
        <description>Vacation Start Date must be earlier than Vacation End Date</description>
        <errorConditionFormula>VREndDateAndTime__c &lt;= VRStartDateAndTime__c</errorConditionFormula>
        <errorMessage>Vacation Start Date must be earlier than Vacation End Date</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Cancelled</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}

var v = new sforce.SObject(&quot;rca__VacationRequest__c&quot;);
v.Id =&quot;{!VacationRequest__c.Id}&quot;;
v.rca__Status__c=&quot;{!VacationRequest__c.Status__c}&quot;;
if (v.rca__Status__c == &quot;Planned&quot;){
   v.rca__Status__c = &quot;Cancelled&quot;;
}
else {
  alert (&quot;You cannot cancel a Vacation Request as it is in progress now. To stop a Vacation Request change Vacation End Date&quot;) 
}
result = sforce.connection.update([v]);
window.location.href=window.location.href;</url>
    </webLinks>
