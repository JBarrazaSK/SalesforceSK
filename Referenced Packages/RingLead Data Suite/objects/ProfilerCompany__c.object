function refresh() {
  location.reload( true );
}

function process( first ) {
  //console.log( &apos;process&apos; );
  var updateRecords = []; //array for holding records that this code will ultimately update 
  var update_Company = new sforce.SObject(&quot;ProfilerCompany__c&quot;); 
  var done = true; 


  update_Company.Id = &apos;{!ProfilerCompany__c.Id}&apos;;
  update_Company.Status__c = &apos;{!ProfilerCompany__c.Status__c}&apos;;
  //console.log( &apos;current status &apos; + update_Company.Status__c );

  if( first === true ) {

    //console.log( &apos;first request&apos; );

    if(  update_Company.Status__c == &apos;Completed&apos; ||  update_Company.Status__c == &apos;Error&apos; || update_Company.Status__c == &apos;New&apos;) {
      //console.log( &apos;requesting&apos; );
      update_Company.Status__c = sforce.apex.execute(&quot;bl_shield.Profiler&quot;, &quot;requestCompany&quot;, {id: &quot;{!ProfilerCompany__c.Id}&quot; });
    }

  }
  else {
     //console.log( &apos;not first request&apos; );
  }

  if( update_Company.Status__c == &apos;In Progress&apos;) {
    //console.log( &apos;checking&apos; );
    update_Company.Status__c = sforce.apex.execute(&quot;bl_shield.Profiler&quot;, &quot;checkCompany&quot;, {id: &quot;{!ProfilerCompany__c.Id}&quot; });
   }

  if( update_Company.Status__c == &apos;Loading&apos;) {
    //console.log( &apos;loading&apos; );
    update_Company.Status__c = sforce.apex.execute(&quot;bl_shield.Profiler&quot;, &quot;loadCompany&quot;, {id: &quot;{!ProfilerCompany__c.Id}&quot; });
  }

  if( update_Company.Status__c == &apos;Error&apos; || update_Company.Status__c == &apos;Completed&apos; ) {
    //console.log( &apos;done&apos; );
    done = true;
  }

  if( update_Company.Status__c == &apos;In Progress&apos; || update_Company.Status__c == &apos;Loading&apos; ) {
    //console.log( &apos;not done&apos; );
    done = false;
  }

 
  //console.log( &apos;new status -  &apos; + update_Company.Status__c );

  if( &apos;{!ProfilerCompany__c.Status__c}&apos;  != update_Company.Status__c) {
    ////console.log( &apos;updating&apos; );
    //updateRecords.push(update_Company); 
    //sforce.connection.update(updateRecords);
    //refresh();
  }

 if( !done ) {
    //console.log( &apos;again&apos; );
    window.setTimeout( process, 1000);
  }

  if( done ) {
    //console.log(&apos;refreshing&apos; );
    alert(&apos; Completed&apos;);
    refresh();
  }
//update_Company.IsUpdateNeeded__c = true;
  //console.log( &apos;finish&apos; );
}


