<apex:page controller="BLDS.CaptureController" >
    
    <apex:form id="changeDescription">
        <apex:styleSheet value="{!URLFOR($Resource.BLDS__ProfilerResources,'Styles/MergeCSS.css')}" />
        <apex:includeScript value="{!URLFOR($Resource.BLDS__ProfilerResources, 'Scripts/MergeJS.js')}" />
        <script>
            /* To show javascript error message in standard VF style*/
            function showError(e)
            {
                afDisplayJSErrorMessage(e.message);
            }
            
            function openPopup(idd, baseURL) // Opens a new popup window and click of view link
            {
                if(idd == 'Create New' || idd == '')
                    alert('You can only view existing accounts.');
                else
                {
                    var tempURL = baseURL+'/'+idd+'/p';
                    window.open(tempURL, "_blank","width=700, height=400, dependent=no, toolbar=no, status=no, directories=no, menubar=no, scrollbars=1, resizable=no", true);
                }   
            }
        </script>
        <!-- div for showing loading icon on top of grid -->                      
        <div id="divButton" style="height:10px;position: absolute;top: 50%;left: 50%;margin-top: -50px;margin-left: -50px;">            
                <apex:actionStatus startText="Loading..." startStyle="color:#ff0000;font-size:16px;font-weight:bold" id="processStatusLoad"> 
                     <apex:facet name="start">
                         <center>
                             <img src="/img/loading.gif" title="Loading..."/>
                         </center>
                     </apex:facet> 
                </apex:actionStatus>
        </div> 
        <apex:actionFunction name="afDisplayJSErrorMessage" action="{displayBlanketError}" rerender="ErrorMessage,errorMessagePnl">
            <apex:param name="jsErrorMsg" value="" assignTo="{!strErrorMessage}"/>
        </apex:actionFunction>
        <!-- A panel to rerender a dummy section -->
        <apex:outputPanel id="tem"/>
        <apex:actionFunction name="rerenderSection" action="{!dummy}" rerender="pbTopSection" />
        <apex:actionFunction name="updateOption" action="{!updateOptionList}" status="processStatusLoad" rerender="pbTopSection, mainPanel2" >
            <apex:param assignTo="{!strSelectedCompanyName}" name="comname" value=""/>
            <apex:param assignTo="{!strSelectedName}" name="Lname" value=""/>
        </apex:actionFunction>  
        <apex:actionFunction name="callCheck" action="{!updateSelected}" rerender="opanel" >
            <apex:param assignTo="{!strRecId}" name="recid" value=""/>
            <apex:param assignTo="{!strFieldName}" name="fname" value=""/>
            <apex:param assignTo="{!strFieldValue}" name="fvalue" value=""/> 
        </apex:actionFunction>
        
        <apex:actionFunction name="reFillMapToUpdate" action="{!reFetchMapToUpdate}" rerender="opanel" > 
            <apex:param assignTo="{!strRecId}" name="CLid" value=""/>
            <apex:param assignTo="{!strSelectAllText}" name="Source" value=""/>
        </apex:actionFunction>  
        <apex:pageMessages id="ErrorMessage" />
        <apex:outputPanel Id="errorMessagePnl">
                <script>
                    /* Section for displaying error message on mouse hover of error icon*/
                    var actualMsg = '{!JSENCODE(strErrorMessage)}';
                    if(actualMsg != '' && actualMsg != null) 
                    {
                        var errorTag =  document.getElementsByClassName('msgIcon');
                        errorTag[0].title = actualMsg;
                    }
                </script>
        </apex:outputPanel>
        <apex:outputPanel id="opanel"/>
        <!-- This section is for displaying Profiler's logo is initial screen is profiler -->
        <apex:outputPanel rendered="{!if(strSectionHeaderSubTitle == 'Profiler Results', true, false)}">
            <table rowspan="0" cellspacing="0" cellpadding="0">
                <tr><td style="height: 14px;"></td></tr>
                <tr>
                    <td>
                        <div id="logo-border">
                            <img src="{!URLFOR($Resource.ProfilerResources,'Images/profiler_powered_logo.png')}" 
                            alt="Broadlook Profiler" id="logo" style="height: 40px; width: 150px;"/>
                        </div>
                    </td>
                </tr>   
                <tr><td style="height: 14px;"></td></tr>   
            </table>    
        </apex:outputPanel>
        <!-- This section is for displaying Capture's logo is initial screen is contact capture -->
        <apex:outputPanel rendered="{!if(strSectionHeaderSubTitle != 'Profiler Results', true, false)}">
            <table rowspan="0" cellspacing="0" cellpadding="0">
                <tr>
                    <td style="height: 14px;"></td>
                </tr>
                <tr>
                    <td style="width:18px;">
                        <apex:image id="theIcon" value="{!URLFOR($Resource.BLDS__ProfilerResources, 'Images/contact_capture_crm_logo.png')}" height="auto"/>
                    </td>
                </tr>
                <tr>
                    <td style="height: 14px;"></td>
                </tr>       
            </table>
        </apex:outputPanel>
        <!-- Main pannel contains all the buttons and data -->
        <apex:outputPanel id="mainPanel1">
            <apex:pageBlock rendered="{!IF(isConfirmPage == false, true, false)}">
                <!-- Button Section -->
                <apex:pageBlockButtons location="Top">
                    <div style="float: left; margin-left:-1%" id="divId">
                        <h style="font-size : 1.5em;"><b>Convert</b></h><br />
                        <h2>Convert your data to a Salesforce entity and merge with any existing records.</h2>                        
                    </div>
                    <script>
                        var temp = document.getElementsByClassName('pbTitle')[0].style.width = '1%';
                        
                        var sectionApp = '{!JSENCODE(strSectionHeaderSubTitle)}';
                        var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
                         if((is_chrome == true || is_chrome == 'true') && sectionApp != 'Profiler Results')
                         { 
                          document.getElementById('divId').style.marginLeft="-2%";
                         }
                    </script>
                    <apex:outputPanel rendered="{!if(strSectionHeaderSubTitle == 'Capture Results', true, false)}" >
                        <apex:commandLink action="{!navigateToContactCapture}" value="Cancel" id="cancelButtonCapture" styleClass="cancelLink"/>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!if(strSectionHeaderSubTitle != 'Capture Results', true, false)}" >
                        <apex:commandLink action="{!navigateToProfiler}" value="Cancel" id="cancelBtnProfiler" styleClass="cancelLink"/> 
                    </apex:outputPanel> 
                    <apex:commandButton action="{!showConfirmPage}" value="Next" id="theButton" style="float:right" rerender="mainPanel1, mainPanel2"/>  
                    <apex:commandButton action="{!redirectToBackPage}" value="Previous" id="backButtonTop" style="float:right;"/>
                    <br/>
                    
                </apex:pageBlockButtons>
                
                    <!-- Displays all the records that are going to be inserted -->
                    <!-- apex:variable var="countContact" value="{!0}" /
                        <apex:variable var="countContact" value="{!countContact+1}"/> 
                    -->
                    <apex:repeat value="{!lstContactInsertIC}" var="ins">
                        
                        <apex:pageBlock id="pbTopSection">
                        <table width="100%">
                            <tr width="100%">
                                <td style="width:50%;"> <b>Name :</b> {!ins.objCon.FirstName}&nbsp;{!ins.objCon.LastName}</td>  
                                <td style="width:50%;"> <b>Company : </b>{!ins.strCompanyName}</td>
                            </tr>
                            <tr>
                            <td></td>
                            <td></td>
                            </tr>
                            <tr>
                                <td> <b>Select {!strAccountLabel} :</b> &nbsp;
                                    <apex:selectList size="1" value="{!ins.strSelectedAccount}" id="check1" onchange="accountChanged('{!$Component.check1}', '{!$Component.hidId}');">
                                        <apex:selectOptions value="{!ins.matchedOptions}" >
                                        </apex:selectOptions>
                                    </apex:selectList>
                                    <apex:inputHidden value="{!ins.strSelectedAccount}" id="hidId"/>
                                    
                                    <a href ="" style="cursor: pointer" onclick="javascript:return openRelatedPopup('{!JSINHTMLENCODE(ins.objCon.LastName)}', '{!JSINHTMLENCODE(ins.strCompanyName)}', '{!JSINHTMLENCODE($Component.hiddenId)}', '{!JSINHTMLENCODE($Component.hiddenName)}')">
                                        <img src="/s.gif" alt="Security Lookup (New Window)" class="lookupIcon" onblur="this.className = 'lookupIcon';"
                                             onfocus="this.className = 'lookupIconOn';" onmouseout="this.className = 'lookupIcon';this.className = 'lookupIcon';"
                                             onmouseover="this.className = 'lookupIconOn';this.className = 'lookupIconOn';" title="Lookup (New Window)" />
                                    </a>
                                    <a title="View (New Window)" style="cursor: pointer; text-decoration: underline;" onClick="openPopup('{!JSINHTMLENCODE(ins.strSelectedAccount)}', '{!JSINHTMLENCODE(strBaseURL)}');">View</a>                                   
                                     <apex:inputField value="{!ins.objCon.AccountId}" id="hiddenId" style="display:none;" />                                     
                                    <apex:inputHidden value="{!ins.strLookupAccountName}" id="hiddenName" />
                                </td>
                                <td>
                                     <apex:outputpanel rendered="{!AND(lstContactInsertIC.size > 1 , ins.theFirst, ins.strSelectedAccount != 'Create New')}">
                                        <apex:commandLink action="{!applyAccountForAllContacts}" value="Apply this Account to all Contacts" id="applyAccountLink" reRender="mainPanel1"/>
                                     </apex:outputPanel>
                                     <apex:actionStatus onstop=""/> 
                                </td>
                            </tr>
                            <!--  Campaign go here ! -->
                            <tr>
                                <td>
                                    <!--
                                    <b>Select Campaign :</b> &nbsp;
                                    <apex:inputText value="{!ins.compaignName}" id="campaignName"/>                                    
                                    
                                    <a href ="" style="cursor: pointer" onclick="javascript:return openRelatedPopup('Campaign', '', '{!$Component.hiddenCampaigId}', '{!$Component.hiddenCampaigName}')">
                                        <img src="/s.gif" alt="Security Lookup (New Window)" class="lookupIcon" onblur="this.className = 'lookupIcon';"
                                             onfocus="this.className = 'lookupIconOn';" onmouseout="this.className = 'lookupIcon';this.className = 'lookupIcon';"
                                             onmouseover="this.className = 'lookupIconOn';this.className = 'lookupIconOn';" title="Lookup (New Window)" />
                                    </a>
                                    <apex:inputHidden value="{!ins.compaignId}" id="hiddenCampaigId"/>
                                    <apex:inputHidden value="{!ins.compaignName}" id="hiddenCampaigName"/>
                                    -->                                                                
                                
                                </td>
                                <td> </td>
                            </tr>
                        </table>
                        </apex:pageBlock>
                    </apex:repeat>
                
                <apex:repeat value="{!objObject}" var="cap">
                </apex:repeat>  
